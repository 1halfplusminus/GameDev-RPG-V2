<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <uie:PropertyField name="GUID" binding-path="GUID" />
    <uie:PropertyField binding-path="WeaponPrefab" name="WeaponPrefab" label="Equipped Prefab" />
    <uie:PropertyField name="Projectile" binding-path="Projectile" />
    <uie:PropertyField name="SocketType" binding-path="SocketType" />
    <uie:PropertyField name="Animation" binding-path="Animation" style="visibility: visible;" />
    <ui:Button text="Update  Animation Data" display-tooltip-when-elided="true" name="UpdateEvent" />
    <ui:Button text="Convert Clip" display-tooltip-when-elided="true" name="ConvertEvent" />
    <uie:PropertyField name="Damage" binding-path="Damage" style="visibility: visible;" />
    <ui:Slider picking-mode="Ignore" label="Damage Modifier Percent" value="0" high-value="100" name="BonusDamagePercent" show-input-field="true" low-value="-100" binding-path="BonusDamagePercent" />
    <uie:PropertyField name="Range" binding-path="Range" />
    <uie:PropertyField name="AttackDuration" binding-path="AttackDuration" readonly="false" />
    <uie:PropertyField name="HitEvents" binding-path="HitEvents" />
    <uie:PropertyField name="Clip" binding-path="BonusDamagePercent" />
</ui:UXML>
